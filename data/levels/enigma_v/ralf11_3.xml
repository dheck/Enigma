    <el:luamain><![CDATA[
ti[" "] = {"fl_space"}
ti["."] = {"fl_swamp"}
ti["W"] = {"st_passage", color=WHITE, flavor="frame"}
if wo["IsDifficult"] then
    ti["-"] = {"fl_red"}
    ti["w"] = ti["."] .. ti["W"]
    ti["^"] = ti["-"] .. ti["W"]
    ti[">"] = ti["-"] .. ti["W"]
    ti["V"] = ti["-"] .. ti["W"]
    ti["<"] = ti["-"] .. ti["W"]
    ti["1"] = ti["-"] .. ti["W"]
    ti["3"] = ti["-"] .. ti["W"]
    ti["7"] = ti["-"] .. ti["W"]
    ti["9"] = ti["-"] .. ti["W"]
else
    ti["-"] = {"fl_metal"}
    ti["w"] = ti["."]
    ti["^"] = {"fl_slope_pn"} .. ti["W"]
    ti[">"] = {"fl_slope_pe"} .. ti["W"]
    ti["V"] = {"fl_slope_ps"} .. ti["W"]
    ti["<"] = {"fl_slope_pw"} .. ti["W"]
    ti["1"] = {"fl_slope_isw"} .. ti["W"]
    ti["3"] = {"fl_slope_ise"} .. ti["W"]
    ti["7"] = {"fl_slope_inw"} .. ti["W"]
    ti["9"] = {"fl_slope_ine"} .. ti["W"]
end
ti["#"] = {"fl_blueslab"} .. ti({"st_rawglass"})
ti["O"] = {"fl_blueslab"} .. ti({"st_oxyd"})
ti["*"] = ti["-"] .. {"#ac_marble_white", essential=INDISPENSIBLE}
ti["@"] = ti["-"] .. {"#ac_marble_black"}

wo(ti, " ", {
    "   ###O######O###   ",
    "   #-1<7----9>3-#   ",
    "   #--@---------#   ",
    "####------------####",
    "#---w..........w---#",
    "#1--............--7#",
    "OV--............--^O",
    "#3--............--9#",
    "#---w..........w---#",
    "####------------####",
    "   #---------*--#   ",
    "   #-7<1----3>9-#   ",
    "   ###O######O###   "})

wo:shuffleOxyd()
 ]]></el:luamain>

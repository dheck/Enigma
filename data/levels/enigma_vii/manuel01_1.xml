function welche_Ecke(aa,bb,cc,dd)
 if aa~=0 and aa~=w_max-1 then return -1 end
 if bb~=0 and bb~=x_max-1 then return -1 end
 if cc~=0 and cc~=y_max-1 then return -1 end
 if dd~=0 and dd~=z_max-1 then return -1 end
 ee = 0
